            @{



                var firstresult = Result.Where(x => x.ParentMenuID == null && x.IsMenu == true).OrderBy(x => x.MenuOrder).ToList();
                foreach (var First in firstresult)
                {
                    <li class="@Html.IsActive(First.MenuName,"Index")">
                        <a href="#Id_@First.MenuID" title="@First.Description" data-toggle="collapse">
                            @{
                                if (First.MenuICon == "Settings")
                                {
                                    <em class="icon-settings"></em>
                                }
                                else if (First.MenuICon == "Purchase")
                                {
                                    <em class="fa fa-shopping-cart"></em>
                                }
                                else if (First.MenuICon == "PayRoll")
                                {
                                    <em class="fa fa-users"></em>
                                }
                                else if (First.MenuICon == "Sale")
                                {
                                    <em class="fa fa-shopping-cart"></em>
                                }
                                else if (First.MenuICon == "Report")
                                {
                                    <em class="icon-chart"></em>
                                }
