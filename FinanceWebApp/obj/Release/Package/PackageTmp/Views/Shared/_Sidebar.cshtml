                                        </a>

                                    }
                                    else
                                    {
                                        <a href="@Url.Action("Index", Second.MenuName)"> @Second.Description</a>
                                    }
                                    <ul id="Id_@Second.MenuID" class="nav sidebar-subnav collapse">

                                        @foreach (var third in thirdresult)
