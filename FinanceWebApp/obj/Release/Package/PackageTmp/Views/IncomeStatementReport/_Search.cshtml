@model IList<MudasirRehmanAlp.Models.StoredPocedureModel.ProcIncomeStatement>

@{

    var counter = 0;
}
<table class="table table-hover" data-page-length='50'>
    <tbody id="tblBody">
        @{
            foreach (var item in Model)
            {
                
                counter = counter + 1;

                <tr>
                    <td>@item.SerialNo</td>
                  
                    <td>@item.AccountName</td>
                    <td>@item.Amount</td>
                   
                </tr>
            }
        }
    </tbody>
</table>
<input type="hidden" value="@counter" id="currentRowID" />
